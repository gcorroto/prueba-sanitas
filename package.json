{
  "name": "prueba-sanitas-monorepo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve angular-ionic-browser",
    "start:commons": "ng build --watch",
    "start:ng": "ng serve ",
    "start:ionic": "ionic serve --project",
    "start:aib": "ng build && npm-run-all \"start:ng -- angular-ionic-browser -o\" ",
    "start:iab": "ng build && npm-run-all \"start:ng -- ionic-angular-browser -o\"",
    "start:iam": "ng build && npm-run-all \"start:ionic -- ionic-angular-mobile\"",
    "start:full": " npm-run-all -l -p \"start:ng -- angular-ionic-browser -o\" \"start:ng -- ionic-angular-browser -o --port 4201\" \"start:ionic -- ionic-angular-mobile --port 4202\"  ",
    "build": "ng build ",
    "build:ios": "ionic cordova build ios --prod",
    "build:android": "ionic cordova build android --prod",
    "build:aib": " npm-run-all \"build -- angular-ionic-browser\" ",
    "build:iab": " npm-run-all \"build -- ionic-angular-browser\"",
    "build:iam": " npm-run-all \"build -- ionic-angular-mobile\"",
    "build:iam:android": " npm-run-all \"build:android -- ionic-angular-mobile\"",
    "build:iam:ios": " npm-run-all \"build:ios -- ionic-angular-mobile\"",
    "build:iam:full": " npm-run-all build:iam build:iam:android build:iam:ios",
    "build:full": "npm-run-all build build:aib build:iab build:iam",
    "t": "ng test --codeCoverage=true --project commons-lib",
    "t:aib": "ng test --codeCoverage=true --project angular-ionic-browser",
    "t:iab": "ng test --codeCoverage=true --project ionic-angular-browser",
    "t:iam": "ng test --codeCoverage=true --project ionic-angular-mobile",
    "t:full": "npm-run-all t t:aib t:iab t:iam",
    "lint": "ng lint",
    "i:aib": "npm i --workspace=angular-ionic-browser",
    "i:iab": "npm i --workspace=ionic-angular-browser",
    "i:iam": "npm i --workspace=ionic-angular-mobile",
    "i:full": "npm i  && npm-run-all i:aib i:iab i:iam "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.10",
    "@angular/common": "~11.2.10",
    "@angular/compiler": "~11.2.10",
    "@angular/core": "~11.2.10",
    "@angular/forms": "^11.2.10",
    "@angular/material": "^11.2.9",
    "@angular/cdk": "^11.2.9",
    "@angular/platform-browser": "~11.2.10",
    "@angular/platform-browser-dynamic": "~11.2.10",
    "@ionic/core": "^5.6.4",
    "@ionic/angular": "^5.5.2",
    "ionicons": "^5.5.1",
    "@angular/router": "~11.2.10",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.9",
    "@angular/cli": "~11.2.9",
    "@angular/compiler-cli": "~11.2.10",
    "@ionic/cli": "^6.13.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "puppeteer": "^8.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
